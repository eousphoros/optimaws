%h1= @loc
%br
%h2 Reserved instances
- @reserved.each do |key,value| 
  %h4= "ReservedID: #{key}"
  %p= "InstanceID: #{value['instanceid']} Duration: #{value['duration']} months @ #{value['offeringType']}"
  %br
%br
%h2 Running instances
- @output.each do |instance, values| 
  %h4= "InstanceID: #{instance}"
  %p= "InstanceType: #{values['instancetype']}, Region: #{values['region']}"
  - values['tags'].each do |key,tags| 
    %p= "Tag: #{key}, Value: #{tags}"
  - GraphFind.all(values['instancetype'], values['region']).each do |file| 
    %object{ :type => "image/svg+xml", :data => "/assets/#{file}", :width => "250", :height => "250"}
